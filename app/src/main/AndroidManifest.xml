<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gkquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/worldlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".score"></activity>
        <activity android:name=".NationalQuiz" />
        <activity android:name=".ParkQuiz"
            android:parentActivityName=".CategoryActivity"></activity>
        <activity android:name=".CategoryActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>